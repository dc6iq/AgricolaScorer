<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
    <style name="GreenTheme" parent="android:Theme.Holo.Light">
        <item name="android:buttonStyle">@style/button</item>
        <item name="android:windowBackground">@drawable/grass</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:listViewStyle">@style/listView</item>
    </style>
    
    <style name="grassBackground">
        <item name="android:background">@drawable/grass</item>
    </style>
    
    <style name="button" parent="android:style/Widget.Holo.Light.Button">
        <item name="android:textColor">#BBFFBB</item>
    </style>
    
    <style name="text">
        <item name="android:textColor">#BBFFBB</item>
    </style>
        
    <style name="listContainer">
        <item name="android:background">@drawable/primary_list_shape</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:padding">10dp</item>
    </style>
    
    <style name="listContainerText">
        <item name="android:textColor">#BBFFBB</item>
        <item name="android:textSize">20dp</item>
        <item name="android:padding">5dp</item>
    </style>
    
    <style name="smallerText" parent="@style/listContainerText">
        <item name="android:textSize">16dp</item>
    </style>
        
    <style name="listTitle" parent="@android:style/TextAppearance.Holo.Large.Inverse">
        <item name="android:paddingBottom">10dp</item>
        <item name="android:textColor">#DDFFDD</item>
    </style>
    
	<style name="historyDate">
	    <item name="android:textSize">20dp</item>
		<item name="android:textColor">#DDFFDD</item>
    </style>
    
    <style name="listView" parent="@android:style/Widget.Holo.Light.ListView">
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="tabWidgetText" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:textColor">#DDFFDD</item>
        <item name="android:background">@drawable/tab_indicator</item>
        <item name="android:padding">15dp</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="segmentedStyle">
        <item name="checkedTextColour">#FFFFFF</item>
        <item name="uncheckedTextColour">#FFFFFF</item>
    </style>
    
    <style name="fieldsStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#8C4D00</item> <!-- light -->
        <item name="checkedEndColour">#6E3700</item> <!-- to dark -->
		<item name="uncheckedStartColour">#8C4D00</item> <!-- light -->
		<item name="uncheckedEndColour">#000000</item> <!-- to black -->
	</style>
	        
    <style name="pasturesStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#1E7E27</item>
        <item name="checkedEndColour">#176114</item>
		<item name="uncheckedStartColour">#1E7E27</item>
		<item name="uncheckedEndColour">#000000</item>
	</style>
	        
    <style name="grainStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#F4E12D</item>
        <item name="checkedEndColour">#FAC020</item>
		<item name="uncheckedStartColour">#F4E12D</item>
		<item name="uncheckedEndColour">#000000</item>
	</style>
		        
    <style name="vegetableStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#FF960F</item>
        <item name="checkedEndColour">#EA7001</item>
		<item name="uncheckedStartColour">#FF960F</item>
		<item name="uncheckedEndColour">#000000</item>
	</style>
		        
    <style name="sheepStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#E7E1D5</item>
        <item name="checkedEndColour">#C4C1BA</item>
		<item name="uncheckedStartColour">#E7E1D5</item>
		<item name="uncheckedEndColour">#000000</item>
	</style>
		        
    <style name="wildBoarStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#8B8883</item>
        <item name="checkedEndColour">#686663</item>
		<item name="uncheckedStartColour">#8B8883</item>
		<item name="uncheckedEndColour">#000000</item>
	</style>
		        
    <style name="cattleStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#E57411</item>
        <item name="checkedEndColour">#A04F11</item>
		<item name="uncheckedStartColour">#E57411</item>
		<item name="uncheckedEndColour">#000000</item>
	</style>
		        
    <style name="familyMemberStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#3592DB</item>
        <item name="checkedEndColour">#136DB5</item>
		<item name="uncheckedStartColour">#3592DB</item>
		<item name="uncheckedEndColour">#000000</item>
	</style>
		        
    <style name="roomTypeStyle" parent="@style/segmentedStyle">
	</style>
	
    <style name="numberPicker">
        <item name="android:textColor">#DDFFDD</item>
    </style>
</resources>